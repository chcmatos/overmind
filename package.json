{
  "name": "overmind",
  "version": "1.0.0",
  "description": "Overmind Simple Form using day/night theme",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass --omit-source-map-url ./assets/scss/ -o ./assets/css/",
    "scss-w": "node-sass -w --omit-source-map-url ./assets/scss/ -o ./assets/css",
    "watch": "npx live-server | (npm run scss-w || watch)"
  },
